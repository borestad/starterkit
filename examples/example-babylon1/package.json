{
  "name": "example-babylon1",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "clean": "rm -rf dist",
    "build": "yarn clean && parcel build index.html",
    "build:js": "babel . --out-dir dist --extensions \".ts,.tsx\" --source-maps true",
    "build:types": "tsc --emitDeclarationOnly",
    "start": "parcel index.html"
  },
  "dependencies": {
    "@babel/cli": "7.1.5",
    "babylonjs": "4.0.0-alpha.10",
    "babylonjs-gui": "3.3.0",
    "babylonjs-loaders": "3.3.0",
    "babylonjs-materials": "3.3.0",
    "babylonjs-viewer": "3.3.0",
    "cannon": "0.6.2",
    "parcel-bundler": "1.10.3",
    "pep": "1.0.13",
    "typescript": "3.1.6"
  }
}
