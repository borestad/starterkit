    "name": "NetEnt AB (publ)",
    "email": "info@netent.com",
    "url": "https://www.netent.com/"
  },
  "engines": {
    "node": ">= 10.x",
    "yarn": ">= 1.12.x",
    "npm": ">= 999-this-repo-is-yarn-only:see-npmrc"
  },
  "main": "src/index.ts",
  "scripts": {
    "build": "del dist && microbundle build src/index.ts -o dist --target web -f umd",
    "clean": "git clean -dXf -e node_modules -e \"*cache*\"",
    "compile": "run-p compile:*",
    "compile:js": "babel src --root-mode upward --verbose --out-dir node_modules/.tmp/build.babel --extensions \".ts,.tsx\" --source-maps inline",
    "compile:ts": "tsc -b tsconfig.json --pretty",
    "lint": "tslint -c ../../tslint.json --project tsconfig.json -t codeFrame --fix",
    "test": "jest -w 2 --notify"
  },
  "files": [
    "dist/*",
    "index.d.ts"
  ],
  "bundledDependencies": [],
  "dependencies": {
    "@babel/core": "7.4.4",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@scope/example-lib1": "*",
    "microbundle": "0.11.0",
    "prettier": "1.17.0",
    "typescript": "3.4.5"
  }
}
