{
  "name": "@scope/example-lib2",
  "private": false,
  "version": "1.0.2",
  "license": "UNLICENSED",
  "author": {
    "name": "NetEnt AB (publ)",
    "email": "info@netent.com",
    "url": "https://www.netent.com/"
  },
  "engines": {
    "node": "10.x",
    "yarn": ">= 1.12.x",
    "npm": ">= this-repo-is-yarn-only:see-npmrc"
  },
  "main": "src/index.ts",
  "scripts": {
    "build": "del dist && microbundle build src/index.ts -o dist --target web -f umd",
    "clean": "git clean -dxf -e node_modules",
    "compile": "run-p -l compile:*",
    "compile:js": "babel src --root-mode upward --verbose --out-dir lib --extensions \".ts,.tsx\" --source-maps inline",
    "compile:ts": "del dist && tsc -b tsconfig.json",
    "dist": "NODE_ENV=production webpack --config ../../config/webpack/index.ts --colors --hide-modules --display-error-details",
    "lint": "tslint -c ../../tslint.json --project tsconfig.json -t codeFrame --fix",
    "test": "jest --notify"
  },
  "files": [
    "dist/*",
    "index.d.ts"
  ],
  "bundledDependencies": [],
  "dependencies": {
    "@babel/core": "7.4.0",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.0",
    "@babel/preset-env": "^7.4.2",
    "@scope/example-lib1": "*",
    "microbundle": "0.11.0",
    "prettier": "1.16.4",
    "typescript": "3.3.4000"
  }
}
