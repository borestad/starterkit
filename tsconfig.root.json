{
  "extends": "./tsconfig.json",
  "include": ["./config/**/*.ts", "./bin/**/*.ts"],
  "compilerOptions": {
    "declaration": false,
    "declarationMap": false,
    "listEmittedFiles": false,
    "listFiles": false,
    "module": "commonjs",
    "moduleResolution": "node",
    "noEmit": true,
    "noLib": false,
    "noUnusedLocals": true,
    "sourceMap": false,
    "target": "es6"
  }
}
