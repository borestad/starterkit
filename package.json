{
  "name": "@netent/starterkit-playground",
  "version": "1.0.0",
  "description": "Starterkit is a `batteries included` base for creating awesome software using Typescript and best practices within Netent",
  "license": "UNLICENSED2",
  "private": true,
  "engines": {
    "node": ">= 10.x",
    "npm": ">= 999-this-repo-is-yarn-only:see-npmrc",
    "yarn": ">= 1.12.x"
  },
  "bugs": {
    "url": "https://jira.netent.com/secure/RapidBoard.jspa?rapidView=1587"
  },
  "homepage": "https://git.netent.com/projects/GS/repos/starterkit",
  "author": {
    "email": "info@netent.com",
    "name": "NetEnt AB (publ)",
    "url": "https://www.netent.com/"
  },
  "workspaces": [
    ".starterkit",
    "examples/*",
    "packages/*"
  ],
  "scripts": {
    "build": "npx lerna run build --parallel --stream",
    "ci": "node_modules/@netent/starterkit/bin/ci",
    "clean": "npx starterkit-clean",
    "compile": "npx run-p compile:*",
    "compile:packages": "npx lerna run compile --parallel --stream",
    "fix": "npx yarn prettier && yarn lint",
    "lint": "npx run-s lint:*",
    "lint:js": "npx eslint . --ext js --ext ts --fix",
    "postinstall": "npx starterkit-postinstall",
    "prettier": "npx prettier -c --write \"**/*.{ts,js,tsx,jsx,json,md,html,css}\"",
    "test": "npx run-p -l test:unit test:e2e",
    "test:e2e": "npx lerna run test:e2e --stream",
    "test:serial": "npx lerna exec -- yarn test",
    "test:unit": "npx jest -w 3 --config=.starterkit/config/jest.config.js",
    "test:watch": "npx yarn test -- --watch --notify",
    "validate": "node node_modules/@netent/starterkit/bin/validate"
  },
  "maintainers": [
    "Johan Borestad <johan.borestad@netent.com>"
  ],
  "prettier": {
    "bracketSpacing": true,
    "jsxBracketSameLine": true,
    "printWidth": 100,
    "semi": false,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "none",
    "useTabs": false
  },
  "dependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "7.5.5",
    "@babel/node": "7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
    "@babel/plugin-transform-runtime": "7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-typescript": "^7.3.3",
    "@babel/register": "7.5.5",
    "@babel/runtime-corejs3": "7.5.5",
    "@types/jest": "24.0.17",
    "@typescript-eslint/eslint-plugin": "1.13.0",
    "@typescript-eslint/parser": "1.13.0",
    "babel-plugin-loop-optimizer": "1.4.1",
    "eslint": "6.1.0",
    "eslint-config-prettier": "6.0.0",
    "eslint-config-standard": "13.0.1",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-node": "9.1.0",
    "eslint-plugin-prettier": "3.1.0",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-sonarjs": "0.4.0",
    "eslint-plugin-standard": "4.0.0",
    "eslint-plugin-unicorn": "10.0.0",
    "jest": "24.8.0",
    "jest-cli": "24.8.0",
    "lerna": "^3.16.4",
    "ts-jest": "24.0.2",
    "ts-node": "8.3.0",
    "type-fest": "0.7.1",
    "typescript": "^3.5.3"
  },
  "devDependencies": {
    "@netent/starterkit": "*"
  }
}
