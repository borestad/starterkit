{
  "name": "@netent/starterkit",
  "description": "Starterkit is a `batteries included` base for creating awesome software using Typescript and best practices within Netent",
  "version": "1.0.0",
  "private": true,
  "license": "UNLICENSED",
  "author": {
    "name": "NetEnt AB (publ)",
    "email": "info@netent.com",
    "url": "https://www.netent.com/"
  },
  "maintainers": [
    "Johan Borestad <johan.borestad@netent.com>"
  ],
  "files": [
    "/lib",
    ".editorconfig",
    ".eslintignore",
    ".eslintrc.js",
    ".gitattributes",
    ".gitconfig",
    ".gitignore",
    ".gitmessage",
    ".huskyrc.js",
    ".lintstagedrc.js",
    ".npmrc",
    ".prettierignore",
    ".yarnrc",
    "Jenkinsfile",
    "LICENSE",
    "README.md",
    "babel.config.js",
    "jenkins.xml",
    "lerna.json",
    "package.json",
    "sonar-project.properties",
    "tsconfig.json",
    "wallaby.js",
    "yarn.lock"
  ],
  "homepage": "https://git.netent.com/projects/GS/repos/starterkit",
  "bugs": {
    "url": "https://jira.netent.com/secure/RapidBoard.jspa?rapidView=1587"
  },
  "engines": {
    "node": ">= 10.x",
    "yarn": ">= 1.12.x",
    "npm": ">= 999-this-repo-is-yarn-only:see-npmrc"
  },
  "workspaces": [
    ".starterkit",
    "examples/*",
    "packages/*"
  ],
  "scripts": {
    "build": "npx lerna run build --parallel --stream",
    "ci": "node_modules/@netent/starterkit/bin/ci",
    "clean": "starterkit-clean",
    "compile": "run-p compile:*",
    "compile:packages": "npx lerna run compile --parallel --stream",
    "fix": "npx yarn prettier && yarn lint",
    "lint": "npx run-s lint:*",
    "lint:js": "npx eslint . --ext js --ext ts --fix",
    "postinstall": "starterkit-postinstall",
    "preinstall": "rm -f .git/hooks/* || true",
    "prettier": "npx prettier -c --write \"**/*.{ts,js,tsx,jsx,json,md,html,css}\"",
    "test": "npx run-p -l test:unit test:e2e",
    "test:unit": "npx jest -w 3",
    "test:e2e": "npx lerna run test:e2e --stream",
    "test:serial": "npx lerna exec -- yarn test",
    "test:watch": "npx yarn test -- --watch --notify",
    "validate": "node node_modules/@netent/starterkit/bin/validate"
  },
  "prettier": {
    "bracketSpacing": true,
    "jsxBracketSameLine": true,
    "printWidth": 80,
    "semi": false,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "none",
    "useTabs": false
  },
  "dependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "7.4.4",
    "@babel/node": "7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.4",
    "@babel/plugin-transform-runtime": "7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@babel/preset-typescript": "^7.3.3",
    "@babel/register": "7.4.4",
    "@babel/runtime-corejs3": "7.4.4",
    "@types/fs-extra": "7.0.0",
    "@types/jest": "24.0.13",
    "@typescript-eslint/eslint-plugin": "1.9.0",
    "@typescript-eslint/parser": "1.9.0",
    "babel-plugin-loop-optimizer": "1.4.1",
    "blend-promise-utils": "1.22.0",
    "chalk": "2.4.2",
    "del-cli": "1.1.0",
    "eslint": "6.0.0-alpha.0",
    "eslint-config-prettier": "4.2.0",
    "eslint-config-standard": "12.0.0",
    "eslint-plugin-cypress": "2.2.1",
    "eslint-plugin-import": "2.17.2",
    "eslint-plugin-node": "9.0.1",
    "eslint-plugin-prettier": "3.1.0",
    "eslint-plugin-promise": "4.1.1",
    "eslint-plugin-sonarjs": "0.3.0",
    "eslint-plugin-standard": "4.0.0",
    "eslint-plugin-unicorn": "8.0.2",
    "esm": "3.2.22",
    "figures": "3.0.0",
    "fs-extra": "8.0.1",
    "husky": "2.2.0",
    "jest": "24.8.0",
    "jest-cli": "24.8.0",
    "lerna": "^3.13.4",
    "lodash": "4.17.11",
    "npm-run-all": "4.1.5",
    "prettier": "1.17.1",
    "ts-jest": "24.0.2",
    "ts-node": "8.1.0",
    "typescript": "^3.4.5",
    "yarn": "1.16.0"
  },
  "devDependencies": {
    "@netent/starterkit": "*",
    "diff-so-fancy": "1.2.5",
    "lint-staged": "8.1.6",
    "node-notifier": "5.4.0"
  }
}
